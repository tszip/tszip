{
  "name": "@tszip/tszip",
  "type": "module",
  "version": "3.1.0",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "description": "Backwards-compatible compiler for TypeScript libraries.",
  "license": "MIT",
  "homepage": "https://github.com/tszip/tszip",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tszip/tszip"
  },
  "bin": {
    "tszip": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup"
  ],
  "bugs": {
    "url": "https://github.com/tszip/tszip/issues"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "preboot": "yarn clean",
    "boot": "tsc && rollup -c --silent && yarn exe",
    "bootstrap": "yarn boot && yarn build",
    "dev": "yarn bootstrap && node dist/index.js dev",
    "build": "node dist/index.js build",
    "postbuild": "yarn exe",
    "exe": "chmod +x dist/index.*js",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/index.js lint ./ --ignore-pattern 'test/e2e/fixtures/lint'",
    "test": "yarn build && node dist/index.js test",
    "prepublish": "yarn deduplicate && yarn bootstrap && yarn test",
    "postpublish": "git push",
    "release": "np",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "deduplicate:check": "yarn-deduplicate -s fewer yarn.lock --list --fail"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/helper-module-imports": "^7.16.7",
    "@babel/parser": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/traverse": "^7.16.7",
    "@optimize-lodash/rollup-plugin": "^3.0.0",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tszip/resolve-imports": "2.6.2",
    "@tszip/rollup-config": "^1.7.0",
    "@types/jest": "^27.4.0",
    "@types/rollup-plugin-postcss": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "ansi-escapes": "^5.0.0",
    "asyncro": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.3",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-polyfill-regenerator": "^0.3.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "camelcase": "^6.3.0",
    "chalk": "^5.0.0",
    "enquirer": "^2.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^6.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "glob-promise": "^4.2.2",
    "jest": "^27.4.7",
    "jest-watch-typeahead": "^1.0.0",
    "jpjs": "^1.2.1",
    "lodash-es": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "ora": "^6.0.1",
    "pascal-case": "^3.1.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "progress-estimator": "^0.3.0",
    "regenerator-runtime": "^0.13.9",
    "resolve": "^1.21.0",
    "resolve.exports": "^1.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "sade": "^1.8.1",
    "semver": "^7.1.1",
    "shelljs": "^0.8.5",
    "tiny-glob": "^0.2.6",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^4.6.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/autoprefixer": "^10.2.0",
    "@types/cssnano": "^5.0.0",
    "@types/eslint": "^7.28.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/sade": "^1.7.4",
    "@types/semver": "^7.3.9",
    "@types/shelljs": "^0.8.10",
    "@types/styled-components": "^5.1.19",
    "@types/tsc-watch": "^4.2.0",
    "autoprefixer": "^10.4.2",
    "babel-plugin-replace-identifiers": "^0.1.1",
    "cssnano": "^5.0.15",
    "doctoc": "^2.1.0",
    "husky": "^7.0.4",
    "np": "^7.6.0",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "styled-components": "^5.3.3",
    "tiny-invariant": "^1.2.0",
    "tiny-warning": "^1.0.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '!test/tests/lint/**' && yarn lint && yarn deduplicate:check && doctoc README.md"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
